<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".services.SearchFragment">

    <include
        android:id="@+id/toolbar"
        layout="@layout/default_toolbar" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/int_const_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/actor"
                android:text="@string/search_frag_title"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <View
                android:id="@+id/view"
                android:layout_width="284dp"
                android:layout_height="82dp"
                android:layout_marginTop="88dp"
                android:background="@drawable/search_page_button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <ImageView
                android:id="@+id/find_pet_image"
                android:layout_width="99dp"
                android:layout_height="62dp"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="16dp"
                android:contentDescription="@string/search_frag_pet_desc"
                android:src="@drawable/hero_conad_petstore"
                app:layout_constraintBottom_toBottomOf="@+id/view"
                app:layout_constraintStart_toStartOf="@+id/view"
                app:layout_constraintTop_toTopOf="@+id/view" />

            <TextView
                android:id="@+id/find_pet_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="54dp"
                android:layout_marginRight="40dp"
                android:fontFamily="@font/actor"
                android:text="@string/search_frag_pet"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/view"
                app:layout_constraintEnd_toEndOf="@+id/view"
                app:layout_constraintHorizontal_bias="0.676"
                app:layout_constraintStart_toEndOf="@+id/imageView6"
                app:layout_constraintTop_toTopOf="@+id/view"
                app:layout_constraintVertical_bias="0.533" />

            <View
                android:id="@+id/view5"
                android:layout_width="284dp"
                android:layout_height="82dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/search_page_button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view" />

            <ImageView
                android:id="@+id/find_pet_sitter_image"
                android:layout_width="98dp"
                android:layout_height="67dp"
                android:layout_marginTop="1dp"
                android:layout_marginStart="15.5dp"
                android:layout_marginLeft="12dp"
                android:src="@drawable/pet_sitter_slide"
                app:layout_constraintBottom_toBottomOf="@+id/view5"
                app:layout_constraintStart_toStartOf="@+id/view5"
                app:layout_constraintTop_toTopOf="@+id/view5"
                app:layout_constraintVertical_bias="0.466" />

            <TextView
                android:id="@+id/find_pet_sitter_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="49dp"
                android:layout_marginRight="32dp"
                android:layout_marginBottom="2dp"
                android:fontFamily="@font/actor"
                android:gravity="center"
                android:text="@string/search_frag_pet_sitter"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintBottom_toBottomOf="@+id/view5"
                app:layout_constraintEnd_toEndOf="@+id/view5"
                app:layout_constraintHorizontal_bias="0.661"
                app:layout_constraintStart_toEndOf="@+id/imageView11"
                app:layout_constraintTop_toTopOf="@+id/view5"
                app:layout_constraintVertical_bias="0.526" />

            <View
                android:id="@+id/view6"
                android:layout_width="284dp"
                android:layout_height="37dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/search_page_button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view5" />

            <ImageView
                android:id="@+id/find_vet_image"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:layout_marginStart="53dp"
                android:layout_marginLeft="48dp"
                android:src="@drawable/pharmacy"
                app:layout_constraintBottom_toBottomOf="@+id/view6"
                app:layout_constraintStart_toStartOf="@+id/view6"
                app:layout_constraintTop_toTopOf="@+id/view6"
                app:layout_constraintVertical_bias="0.454" />

            <TextView
                android:id="@+id/find_vet_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="26dp"
                android:layout_marginRight="32dp"
                android:layout_marginBottom="4dp"
                android:fontFamily="@font/actor"
                android:gravity="center"
                android:text="@string/search_frag_vet"
                android:textColor="@color/black"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@+id/view6"
                app:layout_constraintEnd_toEndOf="@+id/view6"
                app:layout_constraintTop_toTopOf="@+id/view6"
                app:layout_constraintVertical_bias="0.625" />

            <View
                android:id="@+id/view7"
                android:layout_width="284dp"
                android:layout_height="37dp"
                android:layout_marginTop="12dp"
                android:background="@drawable/search_page_button"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/view6" />

            <ImageView
                android:id="@+id/find_shop_image"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:layout_marginTop="4dp"
                android:layout_marginStart="53dp"
                android:layout_marginLeft="48dp"
                android:src="@drawable/shopping_cart"
                app:layout_constraintBottom_toBottomOf="@+id/view7"
                app:layout_constraintStart_toStartOf="@+id/view7"
                app:layout_constraintTop_toTopOf="@+id/view7"
                app:layout_constraintVertical_bias="0.272" />

            <TextView
                android:id="@+id/find_shop_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="33dp"
                android:layout_marginRight="32dp"
                android:layout_marginBottom="4dp"
                android:fontFamily="@font/actor"
                android:gravity="center"
                android:text="@string/search_frag_shop"
                android:textColor="@color/black"
                android:textSize="17sp"
                app:layout_constraintBottom_toBottomOf="@+id/view7"
                app:layout_constraintEnd_toEndOf="@+id/view7"
                app:layout_constraintTop_toTopOf="@+id/view7"
                app:layout_constraintVertical_bias="0.687" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>