<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.AccountRegistration1">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_with_back" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/int_const_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_title"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/subtitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/actor"
                android:gravity="center"
                android:singleLine="false"
                android:text="@string/register2_subtitle"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/title" />

            <TextView
                android:id="@+id/username"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_username"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.051"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/subtitle" />

            <EditText
                android:id="@+id/username_input"
                android:layout_width="330dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_username_input"
                android:inputType="textPersonName"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/username" />

            <TextView
                android:id="@+id/address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_address"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.055"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/username_input" />

            <EditText
                android:id="@+id/address_input"
                android:layout_width="330dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_address_input"
                android:inputType="textPersonName"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/address" />

            <TextView
                android:id="@+id/home_numb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_home_numb"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.056"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/address_input" />

            <EditText
                android:id="@+id/home_numb_input"
                android:layout_width="120dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:layout_marginLeft="15dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_home_numb_input"
                android:inputType="textEmailAddress"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.047"
                app:layout_constraintTop_toBottomOf="@+id/home_numb"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/postal_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginLeft="152dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_postal_code"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.24"
                app:layout_constraintStart_toStartOf="@+id/home_numb"
                app:layout_constraintTop_toBottomOf="@+id/address_input"
                tools:ignore="RtlHardcoded" />

            <EditText
                android:id="@+id/postal_code_input"
                android:layout_width="120dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:layout_marginLeft="175dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_postal_code_input"
                android:inputType="textEmailAddress"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintStart_toStartOf="@+id/home_numb_input"
                app:layout_constraintHorizontal_bias="0.047"
                app:layout_constraintTop_toBottomOf="@+id/postal_code"
                tools:ignore="RtlHardcoded" />

            <TextView
                android:id="@+id/place"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_place"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.045"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/postal_code_input" />

            <EditText
                android:id="@+id/place_input"
                android:layout_width="330dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_place_input"
                android:inputType="textPersonName"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/place" />

            <TextView
                android:id="@+id/province"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_province"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.05"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/place_input" />

            <EditText
                android:id="@+id/province_input"
                android:layout_width="330dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_province_input"
                android:inputType="textPersonName"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/province" />

            <TextView
                android:id="@+id/phone_numb"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_phone_numb"
                android:textColor="@color/black"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.06"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/province_input" />

            <EditText
                android:id="@+id/phone_numb_input"
                android:layout_width="330dp"
                android:layout_height="48dp"
                android:layout_marginTop="4dp"
                android:autofillHints=""
                android:background="@drawable/rounded_rectangle_input"
                android:ems="10"
                android:fontFamily="@font/actor"
                android:hint="@string/register2_phone_numb_input"
                android:inputType="phone"
                android:paddingLeft="8dp"
                android:paddingRight="8dp"
                android:textColorHint="#B0B0B0"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/phone_numb" />

            <CheckBox
                android:id="@+id/pet_sitter_checkBox"
                android:layout_width="200dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp"
                android:buttonTint="@color/black"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_pet_sitter"
                android:textColor="@color/black"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/phone_numb_input"
                tools:targetApi="lollipop" />

            <TextView
                android:id="@+id/cared_pets"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_cared_pets"
                android:textColor="#B0B0B0"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.064"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/pet_sitter_checkBox"
                tools:ignore="TextContrastCheck" />

            <CheckBox
                android:id="@+id/cared_pets_checkBox1"
                android:layout_width="152dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:buttonTint="@color/disable_color"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_cared_pet1"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cared_pets"
                tools:ignore="TextContrastCheck"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/cared_pets_checkBox2"
                android:layout_width="152dp"
                android:layout_height="48dp"
                android:layout_marginStart="150dp"
                android:layout_marginLeft="16dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_cared_pet2"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@+id/cared_pets_checkBox1"
                app:layout_constraintTop_toBottomOf="@+id/cared_pets"
                tools:ignore="TextContrastCheck"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/cared_pets_checkBox3"
                android:layout_width="152dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="35dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_cared_pet3"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cared_pets"
                tools:targetApi="lollipop" />

            <TextView
                android:id="@+id/offered_services"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="9dp"
                android:fontFamily="@font/actor"
                android:text="@string/register2_services"
                android:textColor="#B0B0B0"
                android:textColorHint="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.061"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cared_pets_checkBox3" />

            <CheckBox
                android:id="@+id/services_checkBox1"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_serv1"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/offered_services"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/services_checkBox2"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="35dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_serv2"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/offered_services"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/services_checkBox3"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="70dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_serv3"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/offered_services"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/services_checkBox4"
                android:layout_width="300dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="105dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_serv4"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/offered_services"
                tools:targetApi="lollipop" />

            <CheckBox
                android:id="@+id/services_checkBox5"
                android:layout_width="152dp"
                android:layout_height="48dp"
                android:layout_marginStart="11dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="140dp"
                android:buttonTint="#B0B0B0"
                android:clickable="false"
                android:fontFamily="@font/actor"
                android:minHeight="48dp"
                android:text="@string/register2_serv5"
                android:textColor="#B0B0B0"
                android:textColorHighlight="@color/black"
                android:textColorHint="@color/black"
                android:textColorLink="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/offered_services"
                tools:targetApi="lollipop" />

            <Button
                android:id="@+id/create_account_button"
                android:layout_width="96dp"
                android:layout_height="48dp"
                android:layout_marginTop="35dp"
                android:background="@drawable/rounded_rectangle_app_name"
                android:fontFamily="@font/actor"
                android:text="@string/register2_create_account_button"
                android:textAllCaps="false"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@id/int_const_layout"
                app:layout_constraintTop_toBottomOf="@+id/services_checkBox5" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>